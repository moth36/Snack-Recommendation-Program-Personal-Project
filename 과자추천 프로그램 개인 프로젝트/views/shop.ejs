<body>

<!-- Header Section -->
<div class="header">
  <div class="header-image">
    <div class="dimmer"></div>
    <div class="copy">
      <div class="page-title">
        <h1 class="shop-title">Shop title</h1>
        <p class="subheading">Subheading with description of your shopping site</p>
      </div>
      <button class="button">Button</button>
    </div>
  </div>
</div>

<!-- Navigation Section -->
<div class="navigation">
  <div class="site-name">Site Name</div>
  <div class="items">
    <span class="page">Page</span>
    <span class="page">Page</span>
    <span class="page">Page</span>
    <button class="button">Button</button>
  </div>
</div>

<!-- Filter and snacks Section -->
<div class="page-snacks-results">
  <aside class="filter-menu">
    <div class="section">
      <div class="text">Category</div>
      <div class="keyword-list">
        <div class="tag">Keyword 1</div>
        <div class="tag">Keyword 2</div>
      </div>
    </div>
  </aside>

  <section class="snacks-grid">
    <% snacks.forEach(snacks => { %>
      <div class="snacks-info-card">
        <div class="image" style="background-image: url(<%= snacks.image_url %>)"></div>
        <div class="body">
          <div class="text"><%= snacks.name %></div>
          <div class="text-strong"><%= snacks.price %></div>
        </div>
      </div>
    <% }) %>
  </section>
</div>

<!-- Footer Section -->
<footer class="footer">
  <div class="social-icons">
    <div class="icon">Facebook</div>
    <div class="icon">LinkedIn</div>
    <div class="icon">YouTube</div>
    <div class="icon">Instagram</div>
  </div>
  <div class="site-name">Site Name</div>
</footer>

